<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link
      href="https://cdn.jsdelivr.net/npm/tailwindcss@2.2.19/dist/tailwind.min.css"
      rel="stylesheet"
    />
  </head>
  <body>
    <section class="max-w-2xl mx-auto bg-white p-6 rounded shadow-lg">
      <h2 class="text-2xl font-bold text-blue-700 mb-6">Voting Results</h2>
      <ul>
        <% results.forEach(result => { %>
        <li class="mb-4 p-4 bg-gray-50 rounded">
          <p class="text-gray-800">
            <strong>Party:</strong> <%= result.party %>
          </p>
          <p class="text-gray-800">
            <strong>Vote Count:</strong> <%= result.count %>
          </p>
        </li>
        <% }) %>
      </ul>
    </section>
  </body>
</html>
